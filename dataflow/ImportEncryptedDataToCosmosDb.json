{
	"name": "ImportEncryptedDataToCosmosDb",
	"properties": {
		"description": "Import Encrypted Data to Cosmos",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "VoltageEncryptedCsvData",
						"type": "DatasetReference"
					},
					"name": "EncryptedCsvInput"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CosmosDbNoSqlContainer",
						"type": "DatasetReference"
					},
					"name": "CosmosdbOutput"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          NAME as string,",
				"          AGE as string,",
				"          DOB as string,",
				"          CITY as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EncryptedCsvInput",
				"EncryptedCsvInput sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          eventType as string,",
				"          eventTime as string,",
				"          topic as string,",
				"          subject as string,",
				"          profile as (firstname as string, lastname as string, contact as string),",
				"          dataVersion as string,",
				"          metadataVersion as string,",
				"          data as (validationCode as string, validationUrl as string)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'document',",
				"     partitionKey: ['city'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          NAME,",
				"          AGE,",
				"          DOB,",
				"          CITY",
				"     )) ~> CosmosdbOutput"
			]
		}
	}
}