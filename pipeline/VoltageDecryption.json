{
	"name": "VoltageDecryption",
	"properties": {
		"activities": [
			{
				"name": "ExportDataFromCosmosToCsv",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"preferredRegions": []
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['$name']"
								},
								"sink": {
									"name": "NAME"
								}
							},
							{
								"source": {
									"path": "$['$age']"
								},
								"sink": {
									"name": "AGE"
								}
							},
							{
								"source": {
									"path": "$['$dob']"
								},
								"sink": {
									"name": "DOB"
								}
							},
							{
								"source": {
									"path": "$['$city']"
								},
								"sink": {
									"name": "CITY"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "CosmosDbNoSqlContainer",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "VoltageEncryptedCsvDbExport",
						"type": "DatasetReference",
						"parameters": {
							"filename": {
								"value": "@concat('db_export_', utcnow())",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": []
	}
}